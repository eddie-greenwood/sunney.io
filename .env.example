# Sunney.io Energy Data Platform - Environment Variables
# Copy this file to .env and fill in your values

# Cloudflare Account
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_API_TOKEN=your-api-token

# Storage IDs (create these with wrangler commands)
D1_DATABASE_ID=your-d1-database-id
KV_NAMESPACE_ID=your-kv-namespace-id
R2_BUCKET_NAME=sunney-data

# AEMO Configuration
AEMO_BASE_URL=https://www.nemweb.com.au
SCRAPE_INTERVAL_MINUTES=5

# API Configuration
API_RATE_LIMIT=100
CACHE_TTL_SECONDS=60

# Feature Flags
ENABLE_SCRAPING=true
ENABLE_API=true
DEBUG_MODE=false

# Environment
ENVIRONMENT=development

# Development Database IDs
DEV_D1_DATABASE_ID=your-dev-d1-id
DEV_KV_NAMESPACE_ID=your-dev-kv-id

# Staging Database IDs
STAGING_D1_DATABASE_ID=your-staging-d1-id
STAGING_KV_NAMESPACE_ID=your-staging-kv-id